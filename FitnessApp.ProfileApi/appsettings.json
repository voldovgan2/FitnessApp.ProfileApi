{
  "OpenIdConnect": {
    "Issuer": "http://localhost:5000",
    "Audience": "api_profiles"
  },

  "MongoConnection": {
    "DatabaseName": "FitnessProfiles",
    "CollecttionName": "Profiles"
  },

  "ServiceBus": {
    "Url": "nats://127.0.0.1:4222"
  },

  "VaultClient": {
    "Address": "http://localhost:8200",
    "Token": "dev-only-token"
  },

  "Minio": {
    "Endpoint": "localhost:9000",
    "AccessKey": "minio_user",
    "Secure": "false"
  },

  "GenericFileAggregatorSettings": {
    "ContainerName": "profile",
    "FileFields": [
      "CroppedProfilePhoto",
      "OriginalProfilePhoto",
      "BackgroundPhoto"
    ]
  },

  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },

  "AllowedHosts": "*"
}
